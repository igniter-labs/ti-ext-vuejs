{
  "name": "igniterlabs/ti-ext-vuejs",
  "type": "tastyigniter-extension",
  "description": " Develop stunning, dynamic TastyIgniter themes with the power of Vue.js. ",
  "license": "MIT",
  "authors": [
    {
      "name": "IgniterLabs",
      "email": "hello@igniterlabs.co.uk"
    }
  ],
  "keywords": [
    "igniterlabs",
    "ti-ext-vuejs"
  ],
  "require-dev": {
    "laravel/pint": "^1.2",
    "pestphp/pest-plugin-laravel": "^2.0",
    "pestphp/pest": "^2.5"
  },
  "extra": {
    "tastyigniter-extension": {
      "code": "igniterlabs.vuejs",
      "name": "Vue.js Themes for TastyIgniter",
      "icon": {
        "class": "fa fa-plug",
        "color": "#fff",
        "backgroundColor": "#000"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
